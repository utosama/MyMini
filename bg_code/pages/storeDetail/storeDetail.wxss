/* pages/storeDetail/storeDetail.wxss */
.select-group{
  display: flex;
  width: 100%;
  height: 100rpx;
  border-bottom: 2px solid #F2F2F2;
  border-top: 4px solid #F2F2F2;
}
.item{
  height: 100rpx;
  line-height: 100rpx;
  font-size: 30rpx;
  color: black;
  width: 50%;
  text-align: center;
  background-color: white;
}
.item.active{
  height: 100rpx;
  line-height: 100rpx;
  font-size: 30rpx;
  background-color: #EC693F;
  width: 50%;
  text-align: center;
  color: white;
}
.store-msg-contain{
  display: flex;
  flex-direction: column;
  height: 100rpx;
  width: 100%;
}
.msg-row{
  /* display: flex; */
  height: 50rpx;
  line-height: 50rpx;
  font-size: 28rpx;
  padding-left: 30rpx;
}
.data-cell{
  display: flex;
  flex-direction: column;
  height: 100rpx;
  border-top: 8px solid #F2F2F2;
}
.write-off{
  height: 60rpx;
  line-height: 60rpx;
  font-size: 26rpx;
  margin-left: 30rpx;
  background-color: yellowgreen;
  color: white;
  width: 140rpx;
  text-align: center;
  border-radius: 10rpx;
}
.null{
  font-size: 50rpx;
  text-align: center;
  width: 100%;
  margin-top: 300rpx;
  color: red;
}